---
import Header from "../components/Header.astro";
import Svg from "../components/Svg.astro";
import Name from "../components/Name.astro";
import Content from "../components/Content.astro";
---

<html lang="en">

    <Header />

  <body>

    <Svg />

    <canvas id="grainCanvas"></canvas>

    <Name />

    <img src="/public/assets/down-arrow.svg" alt="Scroll down" class="arrow">


    <Content />

        <img src="/public/assets/up-arrow.svg" alt="Back to top" class="back-to-top" onclick="window.scrollTo({top:0,behavior:'smooth'})">

  </body>
</html>